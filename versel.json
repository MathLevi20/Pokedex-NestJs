{

    "version":2,
    "build":[
        {
            "src":"src/main.ts",
            "use":"@versel/node"
        }
    ],
    "router":[
        {
            "src":"./(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}